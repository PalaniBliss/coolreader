<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="cn.itcreator.android.reader" android:versionCode="1"
	android:versionName="1.0.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">

		
		<activity android:name="CopyOfReaderCanvas"
		android:configChanges="orientation|keyboardHidden|navigation" 
		android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"></action>
			</intent-filter>
		</activity>
		
		
		<activity android:name=".FileBrowser"
		android:configChanges="orientation|keyboardHidden|navigation" 
		android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ImageBrowser"
		android:configChanges="orientation|keyboardHidden|navigation" 
		></activity>
		<activity android:name=".FontSetActivity"
		android:configChanges="orientation|keyboardHidden|navigation"  
		 android:label="@string/changefont"></activity>
	</application>
</manifest> 